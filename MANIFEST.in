# Manifest syntax https://docs.python.org/2/distutils/sourcedist.html
graft wheelhouse

recursive-exclude __pycache__  *.pyc *.pyo *.orig

exclude *.js*
exclude *.git*
exclude *.coveragerc
exclude proc*
exclude pylint*

include requirements/*.txt
include *.py
include wheelhouse

prune .git
prune venv
prune test*
